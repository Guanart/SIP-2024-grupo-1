generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id         Int      @id @default(autoincrement())
  auth0_id   String   @unique
  email      String
  username   String   @db.VarChar(255)
  avatar     String
  createdAt  DateTime @default(now())
  updatedAt  DateTime @updatedAt
}
